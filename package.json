{
  "name": "yalla",
  "version": "1.0.0",
  "description": "community app",
  "main": "index.js",
  "scripts": {
    "start": "npm run lint && npm run setup && npm run compile && node dist/index.js",
    "setup": "npm run migrate && npm run seed",
    "create_migration": "npm run seql migration:generate -- --name $Name",
    "seql": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "create_seeder": "sequelize seed:create --name $Name",
    "seed": "sequelize db:seed:all",
    "migrate": "sequelize db:migrate --migrations-path ./server/core/db/migrations",
    "undo_all_seed": "sequelize db:seed:undo:all",
    "undo_seed": "sequelize db:seed:undo",
    "undo_all_migration": "sequelize db:migrate:undo:all",
    "undo_migration": "sequelize db:migrate:undo",
    "worker-dev": "babel-node ./server/worker.js",
    "worker": "babel-node ./dist/worker.js",
    "watch": "nodemon --exec 'npm run prettify && npm run lint && babel-node server/index.js'",
    "compile": "npm run lint && babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "npm run lint && npm run compile && cross-env NODE_ENV=development node dist/index.js",
    "test": "jest --passWithNoTests",
    "coverage": "jest --env=jsdom --coverage --passWithNoTests --testResultsProcessor ./node_modules/jest-sonar-reporter; exit 0;",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lintfile": "eslint -f json . > report.json ; exit 0;",
    "prettify": "prettier --write server/"
  },
  "dependencies": {
    "@notifications/nodejs": "^1.0.3",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.13.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "google-auth-library": "^7.0.4",
    "jest-sonar-reporter": "^2.0.0",
    "json-refs": "^3.0.15",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "pm2": "^4.5.6",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-enc": "^1.0.8",
    "swagger-express-middleware": "^4.0.0",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.62.0",
    "winston": "^3.2.0",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
